<?xml version="1.0" encoding="UTF-8"?>
<project name="npmpack-maven-plugin" xmlns="http://maven.apache.org/DECORATION/1.4.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/DECORATION/1.4.0 http://maven.apache.org/xsd/decoration-1.4.0.xsd">
    <bannerLeft>
        <name></name>
    </bannerLeft>
    <skin>
        <groupId>org.apache.maven.skins</groupId>
        <artifactId>maven-fluido-skin</artifactId>
        <version>1.3.1</version>
    </skin>
    <custom>
        <fluidoSkin>
            <topBarEnabled>true</topBarEnabled>
            <navBarStyle>navbar-inverse</navBarStyle>
            <topBarIcon>
                <name>Buildbox</name>
                <alt>Buildbox</alt>
                <src>buildbox-logo-wide.svg</src>
                <href>http://github.com/buildbox</href>
            </topBarIcon>
            <sideBarEnabled>true</sideBarEnabled>
            <gitHub>
                <projectId>buildbox/npmpack-maven-plugin</projectId>
                <ribbonOrientation>right</ribbonOrientation>
                <ribbonColor>green</ribbonColor>
            </gitHub>
            <leftColumnClass>span3</leftColumnClass>
            <bodyColumnClass>span9</bodyColumnClass>
            <sourceLineNumbersEnabled>true</sourceLineNumbersEnabled>
        </fluidoSkin>
    </custom>
    <publishDate position="right"/>
    <version position="right"/>

    <body>
        <breadcrumbs>
            <item name="Buildbox" href="http://github.com/buildbox"/>
            <item name="NPMPACK Maven Plugin"
                  href="http://buildbox.github.com/npmpack-maven-plugin/index.html"/>
        </breadcrumbs>

        <menu name="Documentation">
            <item name="Overview" href="index.html"/>
            <item name="Goals" href="plugin-info.html"/>
            <item name="Features" href="features.html"/>
            <item name="Usage" href="usage.html">
                <item name="Publish npm artifact" href="publish-npm.html"/>
            </item>
        </menu>
        <menu name="Links">
            <item name="Apache" href="http://www.apache.org/"/>
            <item name="Maven 2" href="http://maven.apache.org/"/>
            <item name="Google groups" href="http://groups.google.com/group/buildbox"/>
        </menu>
        <menu name="Reports" ref="reports"/>
    </body>
    <poweredBy>
        <logo img="http://maven.apache.org/images/logos/maven-feather.png" 
              href="http://maven.apache.org" 
              name="Apache Maven" position="left"/>
    </poweredBy>
    
</project>
